<% if @errors %>
	<ul>
		<% @errors.each do |error| %>
			<li><%= error %></li>
		<% end %>
	</ul>
<% end %>

<%= form_for :post, url: '/posts' do |f| %>
	
	<%= f.label "Title" %><br>
	<%= f.text_field :title %><br>

	<%= f.label "Content" %><br>
	<%= f.text_area :content %><br>

	<%= f.submit "Post" %>

<% end %>